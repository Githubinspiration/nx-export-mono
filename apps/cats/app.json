{
  "expo": {
    "name": "Cats",
    "slug": "cats",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.exiong.cats"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.exiong.cats"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "serviceWorker": {
        "navigationPreload": true
      },
      "build": {
        "publicUrl": "nx-expo-monorepo"
      },
      "bundler": "metro"
    },
    "plugins": ["@config-plugins/detox"],
    "extra": {
      "eas": {
        "projectId": "a6a590e7-f390-4003-881b-c8ae71dd10b2"
      }
    }
  }
}
