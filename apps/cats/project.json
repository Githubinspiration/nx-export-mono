{
  "name": "cats",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/cats/src",
  "projectType": "application",
  "targets": {
    "start": {
      "executor": "@nx/expo:start",
      "options": {
        "port": 8081
      },
      "dependsOn": ["ensure-symlink", "sync-deps"]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx start cats"
      }
    },
    "run-ios": {
      "executor": "@nx/expo:run",
      "options": {
        "platform": "ios"
      },
      "dependsOn": ["ensure-symlink", "sync-deps"]
    },
    "run-android": {
      "executor": "@nx/expo:run",
      "options": {
        "platform": "android"
      },
      "dependsOn": ["ensure-symlink", "sync-deps"]
    },
    "build": {
      "executor": "@nx/expo:build",
      "options": {},
      "configurations": {
        "preview": {
          "profile": "preview"
        },
        "development": {
          "profile": "development"
        },
        "production": {
          "profile": "production"
        }
      }
    },
    "build-list": {
      "executor": "@nx/expo:build-list",
      "options": {}
    },
    "download": {
      "executor": "@nx/expo:download",
      "options": {
        "output": "apps/cats/dist"
      }
    },
    "sync-deps": {
      "executor": "@nx/expo:sync-deps",
      "options": {}
    },
    "ensure-symlink": {
      "executor": "@nx/expo:ensure-symlink",
      "options": {}
    },
    "prebuild": {
      "executor": "@nx/expo:prebuild",
      "options": {},
      "dependsOn": ["ensure-symlink", "sync-deps"]
    },
    "install": {
      "executor": "@nx/expo:install",
      "options": {}
    },
    "update": {
      "executor": "@nx/expo:update",
      "options": {}
    },
    "submit": {
      "executor": "@nx/expo:submit",
      "options": {}
    },
    "export": {
      "executor": "@nx/expo:export",
      "options": {
        "platform": "all",
        "outputDir": "../../dist/apps/cats"
      },
      "dependsOn": ["ensure-symlink", "sync-deps"]
    },
    "export-web": {
      "executor": "@nx/expo:export",
      "options": {
        "platform": "web",
        "bundler": "metro"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/cats/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/cats/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
